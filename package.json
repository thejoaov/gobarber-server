{
  "name": "gobarber-server",
  "version": "1.1.2",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "predev": "yarn && yarn docker-build:dev && yarn docker-compose:dev -d",
    "dev": "concurrently --kill-others \"nodemon src/server.js\" \"nodemon src/queue.js\" \"serve ./docs\"",
    "server": "nodemon src/server.js",
    "queue": "nodemon src/queue.js",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test jest --forceExit",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all",
    "docker-build:dev": "docker-compose -f docker-compose.dev.yml build --no-cache --pull --force-rm",
    "docker-build:prod": "docker-compose -f docker-compose.prod.yml build --no-cache --pull --force-rm",
    "docker-compose:dev": "docker-compose -f \"docker-compose.dev.yml\" up",
    "docker-compose:prod": "docker-compose -f \"docker-compose.prod.yml\" up"
  },
  "dependencies": {
    "@sentry/node": "5.7.1",
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.2.2",
    "boxen": "^4.2.0",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.0.0-beta.5",
    "dotenv-expand": "^5.1.0",
    "dotenv-extended": "^2.7.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^3.1.0",
    "global": "^4.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.0",
    "nodemailer-express-handlebars": "^3.1.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.15.1",
    "sucrase": "^3.10.1",
    "youch": "^2.0.10",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/jest": "^24.0.21",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "2.0.2",
    "prettier": "1.18.2",
    "sequelize-cli": "^5.5.0",
    "serve": "^11.2.0",
    "sqlite3": "^4.1.0",
    "supertest": "^4.0.2"
  }
}
